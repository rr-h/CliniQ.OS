    "bugs": {
        "url": "https://github.com/osjs/OS.js/issues"
    },
    "homepage": "https://www.os-js.org",
    "engines": {
        "node": ">=10.0.0"
    },
    "eslintConfig": {
        "env": {
            "browser": true,
            "node": true
        },
        "parserOptions": {
            "sourceType": "module",
            "ecmaVersion": 8
        },
        "extends": "@osjs/eslint-config"
    },
    "stylelint": {
        "extends": "@osjs/stylelint-config"
    },
    "babel": {
        "presets": [
            [
                "@babel/preset-env",
                {}
            ]
        ],
        "plugins": [
            "@babel/plugin-transform-runtime"
        ]
    }
}